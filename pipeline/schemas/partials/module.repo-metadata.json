{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "stars": {
      "type": "integer",
      "minimum": 0
    },
    "license": {
      "type": ["string", "null"]
    },
    "hasGithubIssues": {
      "type": ["boolean", "null"]
    },
    "isArchived": {
      "type": ["boolean", "null"]
    },
    "lastCommit": {
      "type": ["string", "null"],
      "format": "date-time"
    }
  }
}
