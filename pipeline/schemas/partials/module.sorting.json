{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["defaultSortWeight", "lastCommit"],
  "properties": {
    "defaultSortWeight": {
      "type": "integer"
    },
    "lastCommit": {
      "type": "string",
      "format": "date-time"
    },
    "keywords": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "string",
        "minLength": 1
      }
    }
  }
}
